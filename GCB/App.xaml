<Application
    x:Class="GCB.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:GCB">
    
    <Application.Resources>
        <ResourceDictionary>
            <x:String x:Key="AppName">GCB</x:String>
            <local:LoginData x:Key="loginData"/>
            <local:InvestmentListData x:Key="investmentListData"/>
            <local:InvestmentDetailsData x:Key="investmentDetailsData"/>
            <local:BranchDetailsData x:Key="branchDetailsData"/>
            <local:DocumentDetailsData x:Key="documentDetailsData"/>
        </ResourceDictionary>
    </Application.Resources>

</Application>
